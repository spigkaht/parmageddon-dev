<div class="map-page-container">
  <div>
    <%# debugger %>
    <h2>Venues in <%= @places_results.first["name"] %></h2>
    <ol>
      <% if @venues == nil %>
        <li>"No venues found!"</li>
      <% else %>
        <% @venues.each do |venue| %>
          <li class="venue-list-item">
            <h4><%= venue.name %></h4>
            <p><%= venue.street %></p>
            <p><%= venue.city %></p>
          </li>
        <% end %>
      <% end %>
    </ol>
  </div>
  <div id="gmaps" data-controller="gmaps"
    data-gmaps-api-key-value="<%= ENV['GOOGLE_MAPS_API_KEY'] %>"
    data-gmaps-venues-value="<%= @venues.to_json %>"
    data-gmaps-location-value="<%= params[:p] %>"
    data-gmaps-target="mapDiv">
  </div>
  <%# <div id="mapbox"
      data-controller="mapbox"
      data-mapbox-api-key-value="<%= ENV['MAPBOX_API_KEY'] "%>
      <%# data-mapbox-markers-value=" @markers.to_json ">%>
  </div>
</div>
